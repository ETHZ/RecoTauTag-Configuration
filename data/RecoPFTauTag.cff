include "TrackingTools/TransientTrack/data/TransientTrackBuilder.cfi"
include "RecoJets/JetAssociationProducers/data/ic5PFJetTracksAssociatorAtVertex.cfi"
#PFTauTagInfo Producer
include "RecoTauTag/RecoTau/data/PFRecoTauTagInfoProducer.cfi"   
#PFTau Producer
include "RecoTauTag/RecoTau/data/PFRecoTauProducer.cfi"
#PFTauDiscriminatorByIsolation Producer
include "RecoTauTag/RecoTau/data/PFRecoTauDiscriminationByIsolation.cfi"
include "RecoTauTag/RecoTau/data/PFRecoTauDiscriminationAgainstElectron.cfi"
  include "RecoTauTag/RecoTau/data/PFRecoTauDiscriminationAgainstMuon.cfi"


#PFRecoTau, Higher efficiency
module pfRecoTauProducerHighEfficiency = pfRecoTauProducer from "RecoTauTag/RecoTau/data/PFRecoTauProducer.cfi"
replace pfRecoTauProducerHighEfficiency.TrackerSignalConeSizeFormula ="5.0/ET"
replace pfRecoTauProducerHighEfficiency.TrackerSignalConeSize_min     = 0.07
replace pfRecoTauProducerHighEfficiency.TrackerSignalConeSize_max     = 0.15
replace pfRecoTauProducerHighEfficiency.GammaCand_minPt = 1.5

module pfRecoTauDiscriminationHighEfficiency = pfRecoTauDiscriminationByIsolation from  "RecoTauTag/RecoTau/data/PFRecoTauDiscriminationByIsolation.cfi"
replace pfRecoTauDiscriminationHighEfficiency.PFTauProducer = "pfRecoTauProducerHighEfficiency"
  
module pfRecoTauDiscriminationHighEfficiencyAgainstElectron = pfRecoTauDiscriminationAgainstElectron from "RecoTauTag/RecoTau/data/PFRecoTauDiscriminationAgainstElectron.cfi"
replace pfRecoTauDiscriminationHighEfficiencyAgainstElectron.PFTauProducer = "pfRecoTauProducerHighEfficiency"

module pfRecoTauDiscriminationHighEfficiencyAgainstMuon = pfRecoTauDiscriminationAgainstMuon from "RecoTauTag/RecoTau/data/PFRecoTauDiscriminationAgainstMuon.cfi"
replace pfRecoTauDiscriminationHighEfficiencyAgainstMuon.PFTauProducer = "pfRecoTauProducerHighEfficiency"

sequence PFTau = {ic5PFJetTracksAssociatorAtVertex,
	 pfRecoTauTagInfoProducer,
	 pfRecoTauProducer,
	 pfRecoTauDiscriminationByIsolation,
	 pfRecoTauDiscriminationAgainstElectron,	
	 pfRecoTauDiscriminationAgainstMuon,
	 pfRecoTauProducerHighEfficiency,
	 pfRecoTauDiscriminationHighEfficiency, 
         pfRecoTauDiscriminationHighEfficiencyAgainstElectron,
	 pfRecoTauDiscriminationHighEfficiencyAgainstMuon

} 
#

